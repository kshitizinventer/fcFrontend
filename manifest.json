{
  "name": "flash cards",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["flashCardScripts.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
